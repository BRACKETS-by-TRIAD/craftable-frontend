{
  "name": "craftable",
  "version": "1.2.15",
  "description": "This is a frontend dependency for Craftable. Craftable is a Laravel-based open-source toolkit for building administration interfaces.",
  "main": "./dist/js/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js": "babel src/js --presets babel-preset-es2015 --out-dir dist/js",
    "copy-scss": "cpx \"src/scss/**/*\" assets/scss",
    "compile-scss": "node-sass --output-style compressed -o dist/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*",
    "build": "npm run js && npm run copy-scss && npm run compile-scss && npm run autoprefixer",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BRACKETS-by-TRIAD/craftable-frontend.git"
  },
  "author": "Brackets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BRACKETS-by-TRIAD/craftable-frontend/issues"
  },
  "homepage": "https://github.com/BRACKETS-by-TRIAD/craftable-frontend#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "jquery": "^3.1.1",
    "moment": "^2.18.1",
    "trumbowyg": "^2.10.0",
    "vee-validate": "2.0.9",
    "vue": "^2.3.4",
    "vue-cookie": "^1.1.4",
    "vue-flatpickr-component": "^2.4.1",
    "vue-js-modal": "^1.2.8",
    "vue-multiselect": "^2.0.2",
    "vue-notification": "^1.3.2",
    "vue-quill-editor": "^2.3.0",
    "vue-trumbowyg": "^3.3.0",
    "vue2-dropzone": "^2.3.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "node-sass": "^4.10.0",
    "postcss-cli": "^6.0.1"
  },
  "files": [
    "/dist",
    "/assets",
    "/coreui"
  ]
}
